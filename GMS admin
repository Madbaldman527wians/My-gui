-- Botões (mantendo visual)
local function criarBotao(nome, texto, posY, url)
    local btn = Instance.new("TextButton", G2L["Anti Detection Scripts_2"])
    btn.Name = nome
    btn.TextWrapped = true
    btn.BorderSizePixel = 0
    btn.TextScaled = true
    btn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    btn.FontFace = Font.new([[rbxasset://fonts/families/Arial.json]], Enum.FontWeight.Bold, Enum.FontStyle.Normal)
    btn.TextColor3 = Color3.fromRGB(193, 193, 193)
    btn.BackgroundTransparency = 1
    btn.Size = UDim2.new(0, 218, 0, 16)
    btn.Text = texto
    btn.Position = UDim2.new(0, 2, 0, posY)
    btn.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet(url))()
    end)
    return btn
end

criarBotao("Options_4", "Anti admin", 66, "https://rawscripts.net/raw/Universal-Script-Anti-chat-admin-32647")
criarBotao("Anti Kick_6", "Anti Kick", 90, "https://raw.githubusercontent.com/ADSKerOffical/AntiCheat/main/Bypass")
criarBotao("Anti Newindex_9", "Newindex Bypass", 114, "https://rawscripts.net/raw/Baseplate-adonis-and-newindex-bypass-source-12378")

-- Tabela de exportação compatível com script principal
local SubGUI = {}

function SubGUI.Toggle()
    G2L["Anti Detection Scripts_2"].Visible = not G2L["Anti Detection Scripts_2"].Visible
end

return SubGUI
