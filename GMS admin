local CollectionService = game:GetService("CollectionService")
local CoreGui = game:GetService("CoreGui")
local G2L = {}

-- ScreenGui principal no CoreGui
G2L["GMSadmin_1"] = Instance.new("ScreenGui", CoreGui)
G2L["GMSadmin_1"].Name = "GMSadmin"
G2L["GMSadmin_1"].ZIndexBehavior = Enum.ZIndexBehavior.Sibling
CollectionService:AddTag(G2L["GMSadmin_1"], "main")

-- Frame Anti Detection Scripts
G2L["Anti Detection Scripts_2"] = Instance.new("Frame", G2L["GMSadmin_1"])
G2L["Anti Detection Scripts_2"].BackgroundColor3 = Color3.fromRGB(255, 255, 255)
G2L["Anti Detection Scripts_2"].AnchorPoint = Vector2.new(0.5, 0)
G2L["Anti Detection Scripts_2"].Size = UDim2.new(0, 418, 0, 132)
G2L["Anti Detection Scripts_2"].Position = UDim2.new(0.5, 12, 0, 226)
G2L["Anti Detection Scripts_2"].Name = "Anti Detection Scripts"
G2L["Anti Detection Scripts_2"].Visible = false -- começa oculto

-- UICorner e UIGradient
G2L["UICorner_5"] = Instance.new("UICorner", G2L["Anti Detection Scripts_2"])
G2L["UICorner_5"].CornerRadius = UDim.new(0, 13)

G2L["UIGradient_7"] = Instance.new("UIGradient", G2L["Anti Detection Scripts_2"])
G2L["UIGradient_7"].Rotation = 80
G2L["UIGradient_7"].Offset = Vector2.new(0, 0.65)
G2L["UIGradient_7"].Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0.000, Color3.fromRGB(0, 0, 0)),
    ColorSequenceKeypoint.new(1.000, Color3.fromRGB(0, 0, 255))
}

-- Título do frame
G2L["Anti Detection Title_8"] = Instance.new("TextLabel", G2L["Anti Detection Scripts_2"])
G2L["Anti Detection Title_8"].TextWrapped = true
